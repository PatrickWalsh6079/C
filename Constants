#include <stdio.h>
#include <stdlib.h>

//

int main()
{

    /*
    constants are variables that cannot be modified
    use keyword const in front of the data type
    best practice is to set constants in all caps
    */

    int num = 5;
    printf("%d\n", num);

    const int NUM = 12;
    printf("%d\n", NUM);

    return 0;
}
