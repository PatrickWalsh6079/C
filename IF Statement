#include <stdio.h>
#include <stdlib.h>

// Use IF statements in your C Function that checks for the largest number

int max(int num1, int num2, int num3){

    int result;

    if(num1 > num2 && num1 > num3){
        result = num1;
    } else if(num2 > num1 && num2 > num3){
        result = num2;
    } else {
        result = num3;
    }

    return result;
}

int main()
{
    printf("The largest number is: %d", max(3,5,10));


    return 0;
}



