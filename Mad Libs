#include <stdio.h>
#include <stdlib.h>

// Build a Mad Libs game in C

int main()
{


    char color[20];
    char plurualNoun[20];
    char celebrity[20];

    printf("Please enter a color: ");
    scanf("%s", color);
    printf("Please enter a plurual noun: ");
    scanf("%s", plurualNoun);
    printf("Please enter a celebrity: ");
    scanf("%s", celebrity);

    printf("Roses are %s\n", color);
    printf("%s are blue\n", plurualNoun);
    printf("I love %s\n", celebrity);

    return 0;
}
